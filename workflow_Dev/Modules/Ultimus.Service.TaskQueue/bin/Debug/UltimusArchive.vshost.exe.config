<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings >
    <!--流程结束后的时间,默认为90天-->
    <add key="IncidentCompleted_Days" value="90"/>
    <!--迁移后的数据库名称-->
    <add key="ArchiveDBName" value="ULTIMUSDB_ARCHIVE"/>
  </appSettings>
  <connectionStrings>
    <add name="UltDB" connectionString="Data Source=.;Initial Catalog=UltimusDB;Integrated Security=True;" />
    <add name="UltArchiveDB" connectionString="Data Source=.;Initial Catalog=ULTIMUSDB_ARCHIVE;Integrated Security=True;" />
    <!--<add name="UltDB" connectionString="Data Source=orcl;User Id=ultimus;Password=ultimus;Integrated Security=no;"  providerName="oracleClient1.0" />-->
  </connectionStrings>
</configuration>